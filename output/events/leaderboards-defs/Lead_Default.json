{
   "gameId": "Fortnite",
   "leaderboardDefId": "Lead_Default",
   "leaderboardStorageId": "Fortnite_GLOBAL",
   "leaderboardInstanceGroupingKeyFormat": "${eventId}",
   "leaderboardInstanceIdFormat": "${windowId}",
   "maxSessionHistorySize": 10,
   "useIndividualScores": false,
   "tiebreakerFormula": {
      "basePointsBits": 11,
      "components": [
         {
            "trackedStat": "LEAD_VICTORY_ROYALE_STAT",
            "bits": 4,
            "aggregation": "sum"
         },
         {
            "trackedStat": "LEAD_TEAM_ELIMS_STAT_INDEX",
            "bits": 12,
            "aggregation": "avg",
            "multiplier": 100
         },
         {
            "trackedStat": "LEAD_PLACEMENT_TIEBREAKER_STAT",
            "bits": 14,
            "aggregation": "avg",
            "multiplier": 100
         },
         {
            "trackedStat": "LEAD_TIME_ALIVE_STAT",
            "bits": 11,
            "aggregation": "avg"
         }
      ]
   },
   "scoringRuleSetId": "EmptyScoringRules",
   "clampsToZero": true,
   "hidePlayerScores": false,
   "percentileAccuracy": 1,
   "requiredPlayerListings": [],
   "discardZeroScore": false
}