{
   "gameId": "Fortnite",
   "leaderboardDefId": "Ranked_Cup_Solo_Gold_LeaderboardDef",
   "leaderboardStorageId": "Fortnite_GLOBAL",
   "leaderboardInstanceGroupingKeyFormat": "${eventId}",
   "leaderboardInstanceIdFormat": "${windowId}_${round}_gold",
   "maxSessionHistorySize": 10,
   "useIndividualScores": false,
   "tiebreakerFormula": {
      "basePointsBits": 11,
      "components": [
         {
            "trackedStat": "GOLD_VICTORY_ROYALE_STAT",
            "bits": 4,
            "aggregation": "sum"
         },
         {
            "trackedStat": "GOLD_TEAM_ELIMS_STAT_INDEX",
            "bits": 12,
            "aggregation": "avg",
            "multiplier": 100
         },
         {
            "trackedStat": "GOLD_PLACEMENT_TIEBREAKER_STAT",
            "bits": 14,
            "aggregation": "avg",
            "multiplier": 100
         },
         {
            "trackedStat": "GOLD_TIME_ALIVE_STAT",
            "bits": 11,
            "aggregation": "avg"
         }
      ]
   },
   "scoringRuleSetId": "ScoringRules_RankedCupSolo_Gold_${round}",
   "clampsToZero": true,
   "payoutsConfig": {
      "payoutTableIdFormat": "${eventId}_${round}_Gold",
      "payoutDate": "windowEnd+PT32M"
   },
   "hidePlayerScores": false,
   "percentileAccuracy": 1,
   "requiredPlayerListings": [],
   "discardZeroScore": true
}